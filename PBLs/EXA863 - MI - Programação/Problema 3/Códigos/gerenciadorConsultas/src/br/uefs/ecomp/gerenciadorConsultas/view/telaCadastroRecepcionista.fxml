<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.uefs.ecomp.gerenciadorConsultas.controller.TelaCadastroRecepcionistaController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Nome do usuário" />
            <TextField fx:id="nomeRecepcionista" promptText="Nome ">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </TextField>
            <Label text="CPF" />
            <TextField fx:id="cpfRecepcionista" promptText="CPF (somente números)">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </TextField>
            <Label text="Senha" />
            <PasswordField fx:id="senhaRecepcionista" promptText="Senha">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </PasswordField>
            <Label text="Confrime a senha" />
            <PasswordField fx:id="confirmacaoSenhaRecep" promptText="Digite a senha novamente">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></PasswordField>
            <HBox alignment="CENTER" prefHeight="37.0" prefWidth="400.0" spacing="200.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <children>
                  <Button fx:id="btnCancelar" cancelButton="true" mnemonicParsing="false" onAction="#clickCancelar" text="Cancelar" />
                  <Button alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#clickConcluido" text="Concluído" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="100.0" right="100.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>

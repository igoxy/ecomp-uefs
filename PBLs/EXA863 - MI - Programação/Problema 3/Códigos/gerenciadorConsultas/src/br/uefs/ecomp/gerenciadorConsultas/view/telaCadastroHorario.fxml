<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="216.0" prefWidth="251.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.uefs.ecomp.gerenciadorConsultas.controller.TelaCadastroHorarioController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="333.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="22.0" prefWidth="134.0" text="Insira o horário">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="19.0" prefWidth="600.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" prefHeight="18.0" prefWidth="40.0" text="Horas" />
                  <Label alignment="CENTER" prefHeight="18.0" prefWidth="46.0" text="Minutos" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="28.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="textHoras" alignment="CENTER" prefHeight="26.0" prefWidth="46.0" promptText="HH" />
                  <Label prefHeight="26.0" prefWidth="2.0" text=":">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <TextField fx:id="textMinutos" alignment="CENTER" prefHeight="26.0" prefWidth="46.0" promptText="MM" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="40.0" prefWidth="251.0" spacing="20.0">
               <children>
                  <Button cancelButton="true" mnemonicParsing="false" onAction="#clickCancelar" text="Cancelar" />
                  <Button fx:id="btnConcluido" defaultButton="true" mnemonicParsing="false" onAction="#clickConcluido" text="Concluído" />
               </children>
               <padding>
                  <Insets top="50.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
